    <Type Name="Slang::UIntSet">
        <DisplayString>{{max_size={m_buffer.m_count*Slang::UIntSet::kElementSize}}}</DisplayString>
        <Expand>
            <Synthetic Name="[Values]">
                <Expand>
                    <CustomListItems MaxItemsPerView="1000">
